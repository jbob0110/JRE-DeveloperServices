{  
    "name":"Jira Extension - Developer Services",
    "version":"0.1.0",
    "permissions":[
       "activeTab",
       "tabs",
       "storage",
       "scripting"
    ],
    "host_permissions": [
      "https://*/"
    ],
    "action": {
       "default_popup": "popup.html",
       "default_icon": {
          "16":"icons/icon16-light.png",
          "48":"icons/icon48-light.png",
          "128":"icons/icon128-light.png"
       }
    },
    "manifest_version":3
 }
